<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
 PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- 			별명                                         실제 객체 -->
		<typeAlias alias="auction" type="com.alpaca.alpacaAuction.model.Auction" />
		<typeAlias alias="bid" type="com.alpaca.alpacaAuction.model.Bid" />
		<typeAlias alias="review_board" type="com.alpaca.alpacaAuction.model.ReviewBoard"/>
		<typeAlias alias="review_reply" type="com.alpaca.alpacaAuction.model.ReviewReply"/>
		<typeAlias alias="member" type="com.alpaca.alpacaAuction.model.Member"/>
	</typeAliases>
	<mappers>
		<mapper resource="auction.xml" />
		<mapper resource="bid.xml" />
		<mapper resource="review_board.xml"/>
		<mapper resource="review_reply.xml"/>
		<mapper resource="member.xml" />
	</mappers>
</configuration>